{
  "name": "nvt-utilidades",
  "version": "0.0.0",
  "description": "A collection of commands",
  "repository": {
    "type": "git",
    "url": "https://github.com/Novout/ut"
  },
  "bugs": {
    "url": "https://github.com/Novout/ut/issues"
  },
  "homepage": "https://github.com/Novout/ut/",
  "author": "Giovane Silva",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "package.json",
    "LICENSE"
  ],
  "keywords": [
    "git",
    "cli"
  ],
  "scripts": {
    "dev": "pnpm build && node dist/index.js",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "format": "prettier . --write",
    "prepublishOnly": "pnpm build",
    "patch": "generi log patch",
    "minor": "generi log minor",
    "major": "generi log major"
  },
  "bin": {
    "nvt-utilidades": "dist/index.js"
  },
  "dependencies": {
    "bumpp": "^10.3.1",
    "destr": "^2.0.5",
    "execa": "9.6.0",
    "fs-extra": "^11.3.2",
    "js-yaml": "^4.1.0",
    "pathe": "2.0.3",
    "sade": "^1.8.1",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.0",
    "generi": "^2.0.4",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
